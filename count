#include<stdio.h>

#include<conio.h>

int main()

{

int min, max, i, j, flag, count=0;

printf("enter minimum number");

scanf("%d",&min);

printf("enter maximum number");

scanf("%d",&max);

for(i=min, i<=max; i++)

{

flag=0;

for(j=2; j<=i/2; j++)

{
if(i%j==0)

flag=1;

break;

}

}

if( flag==0 & & i>=2)

{

printf ("%d \t",i);

count++;

}

}

printf(" \n  prime count=%d", count);

getch( );

return 0;

}

 

 

 
